<tr>
    <td>
        <a href="/sessionAttendance?date={{ date }}&session={{ session }}"
            >{{ date }}</a
        >
    </td>
    <td>
        <div class="session-type" id="{{session}}">{{ session }}</div>
    </td>
    <td class="right-align">
        <button
            type="button"
			class="delete fa-button"
			aria-label="delete-button"
            data-date = {{ date }}
            data-session = {{ session }}
        >
            <i class="fa-solid fa-trash"></i>
        </button>
    </td>
</tr>